<!DOCTYPE html>
<html lang="en">
<head>
    <meta http-equiv="content-type" content="text/html; charset=UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1, user-scalable=no">
    <meta charset="utf-8">
    <title>Xlight Bootstrap Responsive HTML5/CSS3 Template</title>
    <meta name="description" content="Xlight Bootstrap Responsive HTML5/CSS3 Template">
    <meta name="author" content="Andsolutions.it">
    <meta name="description" content="">

    <link href="/css/bootstrap.min.css" rel="stylesheet">
    <link href="/css/bootstrap-theme.min.css" rel="stylesheet">
    <link href="/css/animate.min.css" rel="stylesheet">
    <link href="/css/font-awesome.min.css" rel="stylesheet">
    <link href="/css/prettyPhoto.css" rel="stylesheet">

    <link href="/css/theme.css" rel="stylesheet">
    <link href="/css/responsive.css" rel="stylesheet">
    <link href="/css/colors/blue.css" rel="stylesheet" class="colors">

    <!-- Google Font -->
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Open+Sans:wght@400;700&display=swap" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css2?family=Open+Sans:wght@300;700&display=swap" rel="stylesheet">

    <!-- Favicons -->
    <link rel="shortcut icon" href="/images/ico/favicon.ico">
    <link rel="apple-touch-icon" href="/images/ico/apple-touch-icon.png">
    <link rel="apple-touch-icon" sizes="72x72" href="/images/ico/apple-touch-icon-72.png">
    <link rel="apple-touch-icon" sizes="114x114" href="/images/ico/apple-touch-icon-114.png">
    <link rel="apple-touch-icon" sizes="144x144" href="/images/ico/apple-touch-icon-144.png">

</head>

<body data-spy="scroll" data-target="#mynav" data-offset="85">

    <!-- Preloader -->
    <div id="preloader">
        <div id="status">
            <div class="spinner">
                <div class="rect1"></div>
                <div class="rect2"></div>
                <div class="rect3"></div>
                <div class="rect4"></div>
                <div class="rect5"></div>
            </div>
        </div>
    </div>
    <!-- End Preloader -->

    <header>

        <!-- Bootstrap Menu -->
        <div id="navigation">
            <div class="navbar-wrapper scroll-fixed-navbar">
                <nav class="navbar-inverse navbar-static-top" role="navigation">
                    <div class="container">
                        <div class="row">
                            <div class="navbar-header">
                                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                                    <span class="icon-bar"></span>
                                    <span class="icon-bar"></span>
                                    <span class="icon-bar"></span>
                                </button>
                                <a class="navbar-brand" href="#"></a>
                            </div>
                            <div id="mynav" class="navbar-collapse collapse">
                                <ul class="nav navbar-nav main-nav-list">
                                    <li><a asp-controller="Home" asp-action="Index">Anasayfa</a></li>
                                    <li><a asp-controller="Ajan" asp-action="List">Ajanlar</a></li>
                                    <li><a asp-controller="Silah" asp-action="List">Silahlar</a></li>
                                    <li><a asp-controller="Harita" asp-action="List">Haritalar</a></li>
                                    <li><a asp-controller="Haber" asp-action="List">Haberler</a></li>
                                    <li><a asp-controller="Home" asp-action="Forum">Forum</a></li>
                                    <li><a asp-controller="Home" asp-action="Iletisim">İletişim</a></li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </nav>
            </div>
        </div>
        <!-- End Bootstrap Menu -->
        <!-- Slider -->
        <section id="home">
            <div class="slide-wrap">
                <div id="home-slider" class="carousel slide carousel-fade" data-ride="carousel">
                    <div class="carousel-inner">
                        <div class="item active">
                            <!-- Change Image -->
                            <video width="100%" height="100%" preload="1" muted autoplay>
                                <source src="/videos/baslangic.mp4" type="video/mp4">
                            </video>
                            <div class="caption">
                                <h1 class="animated fadeInLeftBig">VALORANT<strong> WIKI</strong></h1>
                                <p class="animated fadeInRightBig">Valorant Wiki ye Hoşgeldin :)</p>
                                <a data-scroll class="learn-more animated fadeInUpBig" href="#about">Keşfetmeye Başla</a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
        <!-- End Slider -->

    </header>

    @RenderBody()
    <!-- Footer -->
    <div class="bottom-footer">
        <div class="container">
            <div class="bottom-footer-center wow fadeInDown" data-wow-duration="1s" data-wow-delay="300ms">
                <ul class="bottom-social-icons">
                    <li><a target="_blank" href="https://www.facebook.com/seyit3481/"><i class="fa fa-facebook"></i></a></li>
                    <li><a target="_blank" href="https://twitter.com/omerozcelik34"><i class="fa fa-twitter"></i></a></li>
                    <li><a target="_blank" href="https://github.com/omerozcelik34"><i class="fa fa-github"></i></a></li>
                    <li><a target="_blank" href="https://www.instagram.com/omerozcelik34/?next=%2Fomerozcelik34%2F"><i class="fa fa-instagram"></i></a></li>
                    <li><a target="_blank" href="https://www.linkedin.com/in/seyit-ömer-özçelik-577a47236/"><i class="fa fa-linkedin"></i></a></li>
                </ul>
            </div>
            <div class="bottom-footer-left wow fadeInUp" data-wow-duration="1s" data-wow-delay="450ms">
                <p><span>&#169; 2023 <strong>Seyit Ömer Özçelik</strong> Tarafından Geliştirildi</span></p>
            </div>
        </div>
    </div>
    <!-- End Footer -->
    <!-- Scroll to Top  -->
    <a href="#" class="scroll-up"><i class="fa fa-arrow-up"></i></a>
    <!-- End Scroll To Top  -->
    <!-- Bootstrap core JavaScript
    ================================================== -->
    <!-- Placed at the end of the document so the pages load faster -->
    <script src="/js/jquery.min.js"></script>
    <script src="/js/bootstrap.min.js"></script>
    <script src="/js/retina.min.js"></script>
    <script src="/js/jquery.easing.min.js"></script>
    <script src="/js/wow.min.js"></script>
    <script src="/js/waypoints.min.js"></script>
    <script src="/js/jquery.countTo.js"></script>
    <script src="/js/jquery.mixitup.min.js"></script>
    <script src="/js/jquery.prettyPhoto.js"></script>
    <script src="/js/jquery.knob.min.js"></script>
    <script src="/js/jquery.validate.min.js"></script>
    <script src="/js/custom.js"></script>


</body>
</html>